-align: center;\n  padding: 20px 20px;\n  box-shadow: 0px 10px 11px rgba(0, 0, 0, 0.25);\n  border-radius: 5px;\n  background-color: #ffffff;\n  width: 190px;\n  font-size: 0.75em;\n}\n\n.container-education-box p:first-child,\np:last-child {\n  color: #867c7c;\n}\n\n.continue-education-certification {\n  display: flex;\n  text-align: center;\n  background-color: #ffffff;\n  font-size: 0.75em;\n}\n \n\nhr {\n  border-top: 1px dashed #bab3b3;\n} \n\n.continue-education-certification div {\n  padding: 10px;\n  flex: 1;\n  align-self: center;\n}\n  \na {\n  font-weight: 100;\n  color: #131212;\n}\n\n.continue-education-institution {\n  color: #867c7c;\n}\n\n.container-education  {\n  display: flex;\n  gap: 15px;\n  margin-top: 35px;\n}\n\n.container-formacao {\n  display: flex;\n  gap: 15px;\n  margin-top: 35px;\n}\n\n.container-continue-education {\n  display: flex;\n  box-shadow: 0px 10px 11px rgb(0 0 0 / 25%);\n  margin-bottom: 25px;\n width: 100%;\n}\n\n.alguns-cursos {\n  display: flex;\n  box-shadow: 0px 10px 11px rgb(0 0 0 / 25%);\n  margin-bottom: 25px;\n width: 100%;\n}\n\n.container-continue-education {\n  background-color: #ffffff;\n  padding: 10px;\n  border-radius: 5px;\n  width: 100%;\n}\n\n.box-alguns-cursos {\n  background-color: #ffffff;\n  padding: 10px;\n  border-radius: 5px;\n  width: 100%;\n}\n\n\n\n\n.education-description-detail {\n  height: 20px;\n  align-self: center;\n  border-right: 4px solid #bab3b3;\n   margin: -15px;\n}\n\n.certification-link:hover{\n color: blue !important;\n text-decoration: underline;\n}\n\n@media screen and (max-width: 1022px) {\n .container-rota-curso {\n    flex-direction: column;\n  }\n  .section-education {\n        margin-top: 20px;\n  }\n  .container-education {\n    flex-wrap: wrap;\n     flex-direction: column;\n     justify-content: center;\n     align-items: center;\n  }\n  .education-description-detail{\n    display: none;\n  }\n.container-continue-education {\n  width: 100%;\n}\n.container-continue-educatio